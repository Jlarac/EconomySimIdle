; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="EconomySimIdle"
run/main_scene="res://Scenes/UI/title_screen.tscn"
config/features=PackedStringArray("4.4", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/fullsize=false
boot_splash/image="uid://bhno1x13k7jk"
config/icon="uid://d0lqh0kjcihg5"
boot_splash/minimum_display_time=1000

[autoload]

Data="*res://Scripts/Game/Data.gd"
Characters="*res://Scripts/Classes/characters.gd"
GlobalTime="*res://Scripts/Classes/time.gd"
BackgroundMusic="*res://Scenes/UI/background_music.tscn"

[display]

